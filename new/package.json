{
  "name": "@corastuff/new",
  "private": true,
  "packageManager": "pnpm@10.25.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:convex": "pnpm --filter @corastuff/web exec convex dev",
    "dev:web": "pnpm --filter @corastuff/web dev",
    "build:web": "pnpm -r --filter @corastuff/shared --filter @corastuff/web build",
    "dev:worker": "pnpm --filter @corastuff/worker dev",
    "build:worker": "pnpm -r --filter @corastuff/shared --filter @corastuff/queue --filter @corastuff/scrapers --filter @corastuff/worker build",
    "build": "pnpm -r build",
    "typecheck": "pnpm -r --filter @corastuff/shared --filter @corastuff/queue --filter @corastuff/scrapers build && pnpm -r typecheck",
    "lint": "echo \"(lint not configured yet)\"",
    "format": "echo \"(format not configured yet)\""
  },
  "devDependencies": {
    "bullmq": "^5.66.0",
    "convex": "^1.31.0",
    "ioredis": "^5.8.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "msgpackr-extract"
    ]
  },
  "dependencies": {
    "fuse.js": "^7.1.0"
  }
}
